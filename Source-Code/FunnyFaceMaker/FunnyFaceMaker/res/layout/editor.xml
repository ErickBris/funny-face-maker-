<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/rlay1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/appmain" >

    <RelativeLayout
        android:id="@id/draglayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/linearLayout2"
        android:layout_below="@id/horizontalScrollView1" >

        <change.look.face.DragLayer
            android:id="@id/drag_layer"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            
            <ImageView
            android:id="@+id/frame"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@id/drag_layer"
            android:src="@drawable/appmain" />
        </change.look.face.DragLayer>

    </RelativeLayout>

    <HorizontalScrollView
        android:id="@id/horizontalScrollView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:scrollbars="none" >

        <LinearLayout
            android:id="@id/LinearLayout1"
            android:layout_width="830.0dip"
            android:layout_height="wrap_content" >

            <ImageButton
                android:id="@id/Glips"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/lips" />

            <ImageButton
                android:id="@id/geye"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/eye" />

            <ImageButton
                android:id="@id/Ghead"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/topi" />

            <ImageButton
                android:id="@id/Gdadhi"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/dadhi" />

            <ImageButton
                android:id="@id/Ggoggles"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/goggle0" />

            <ImageButton
                android:id="@id/Gnose"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/nose" />

            <ImageButton
                android:id="@id/Gtie"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/tie" />

            <ImageButton
                android:id="@id/Gear"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/ear" />

            <ImageButton
                android:id="@id/Gcigar"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/cigar" />

            <ImageButton
                android:id="@id/Ghair"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/hair" />

            <ImageButton
                android:id="@id/Gmask"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/masks" />

            <ImageButton
                android:id="@id/Ghorror"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rouned_button"
                android:src="@drawable/horror" />
        </LinearLayout>
    </HorizontalScrollView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@id/done"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10.0dip"
            android:background="@drawable/done" />

        <ImageButton
            android:id="@id/delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10.0dip"
            android:background="@drawable/close" />
    </LinearLayout>

</RelativeLayout>